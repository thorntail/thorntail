{
  "openapi": "3.0.0",
  "components": {
    "schemas": {
	    "ErrorModel": {
	      "type": "object",
	      "required": [
	        "message",
	        "code"
	      ],
	      "properties": {
	        "message": {
	          "type": "string"
	        },
	        "code": {
	          "type": "integer",
	          "minimum": 100,
	          "maximum": 600
	        }
	      }
	    },
	    "ExtendedErrorModel": {
	      "allOf": [
	        {
	          "$ref": "#/definitions/ErrorModel"
	        },
	        {
	          "type": "object",
	          "required": [
	            "rootCause"
	          ],
	          "properties": {
	            "rootCause": {
	              "type": "string"
	            }
	          }
	        }
	      ]
	    }
    }
  }
}
