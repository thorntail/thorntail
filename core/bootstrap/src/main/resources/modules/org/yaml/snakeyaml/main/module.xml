<module xmlns="urn:jboss:module:1.3" name="org.yaml.snakeyaml">
  <properties>
    <property name="jboss.api" value="private"/>
  </properties>
  <dependencies>
    <system export="true">
      <paths>
        <path name="org/yaml/snakeyaml"/>
        <path name="org/yaml/snakeyaml/composer"/>
        <path name="org/yaml/snakeyaml/constructor"/>
        <path name="org/yaml/snakeyaml/emitter"/>
        <path name="org/yaml/snakeyaml/error"/>
