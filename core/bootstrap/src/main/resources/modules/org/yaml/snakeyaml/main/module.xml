        <path name="org/yaml/snakeyaml/parser"/>
        <path name="org/yaml/snakeyaml/reader"/>
        <path name="org/yaml/snakeyaml/representer"/>
        <path name="org/yaml/snakeyaml/resolver"/>
        <path name="org/yaml/snakeyaml/scanner"/>
        <path name="org/yaml/snakeyaml/serializer"/>
        <path name="org/yaml/snakeyaml/tokens"/>
        <path name="org/yaml/snakeyaml/util"/>
      </paths>
    </system>
  </dependencies>

